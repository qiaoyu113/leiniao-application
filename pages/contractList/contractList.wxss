/* pages/contractList/contractList.wxss */
.contractList{
  background: #F2F2F2;
  min-height: 100vh;
}
.van-tabs__line{
  background-image: linear-gradient(270deg, #EFAB49 0%, #FFE172 100%);
  border-radius: 20px;  
  height: 10rpx;
}
.listBox{
  padding: 10rpx 15rpx;
  box-sizing: border-box;
}
.noData{
  height: calc(100vh - 350rpx);
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.custom-image .van-empty__image {
  width: 379rpx;
  height: 364rpx;
}
/* 弹窗 */
.modal-mask {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  opacity: 0.5;
  overflow: hidden;
  z-index: 9000;
  color: #fff;
}
.modal-dialog {
  width: 540rpx;
  overflow: hidden;
  position: fixed;
  top: 50%;
  left: 0;
  z-index: 9999;
  background: #f9f9f9;
  margin: -300rpx 105rpx;
  border-radius: 36rpx;
}
.modal-title {
  padding-top: 50rpx;
  font-size: 36rpx;
  color: #030303;
  text-align: center;
}

.modal-footer {
  display: flex;
  flex-direction: row;
  height: 86rpx;
  border-top: 1px solid #dedede;
  font-size: 34rpx;
  line-height: 86rpx;
}
.btn-cancel {
  width: 50%;
  color: #666;
  text-align: center;
  border-right: 1px solid #dedede;
}
.btn-confirm {
  width: 50%;
  color: #ec5300;
  text-align: center;
}
.modal-box{
  display: flex;
  flex-direction: column;
  padding: 50rpx 32rpx;
}
.modal-box text{
  font-size: 28rpx;
  color: #666;
  margin: 10rpx 0;
}