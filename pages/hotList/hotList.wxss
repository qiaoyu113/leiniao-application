@import '/lib/css/layout.wxss';
@import '/lib/css/themeBase.wxss';
.list_box {
  width: 690rpx;
  margin: 40rpx 35rpx;
}
.top_banner {
  width: 100%;
  height: 246rpx;
  margin-bottom: 40rpx;
}

.left_img {
  width: 200rpx;
  height: 200rpx;
  margin-right: 50rpx;
}
.right_list {
  width: 430rpx;
  height: 180rpx;
}
.list_title {
  display: block;
  width: 430rpx;
  height: 40rpx;
  font-size: 28rpx;
  font-weight: 500;
  color: #333333;
  line-height: 40rpx;
  margin-bottom: 27rpx;
  overflow: hidden;
}
.list_sub_lefttitle {
  display: block;
  float: left;
  width: 175rpx;
  height: 33rpx;
  font-size: 24rpx;
  font-weight: 500;
  color: #999999;
  line-height: 33rpx;
  margin-right: 25rpx;
  margin-bottom: 5rpx;
  overflow: hidden;
}
.list_sub_righttitle {
  display: block;
  float: left;
  width: 180rpx;
  height: 33rpx;
  font-size: 24rpx;
  font-weight: 500;
  color: #999999;
  line-height: 33rpx;
  margin-right: 50rpx;
  margin-bottom: 5rpx;
  overflow: hidden;
}
.hidden_word {
  display: block;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.bottom_list {
  width: 690rpx;
  height: 159rpx;
  background: #f9f9f9;
  border-radius: 20rpx solid #f9f9f9;
  margin: 0 auto;
  overflow: hidden;
  padding-top: 25rpx;
  padding-left: 30rpx;
  padding-right: 36rpx;
  box-sizing: border-box;
}
.bottom_img {
  display: inline-block;
  width: 28rpx;
  height: 30rpx;
  margin-right: 19rpx;
}
.bottom_title {
  display: inline-block;
  width: 196rpx;
  height: 40rpx;
  font-size: 28rpx;
  font-weight: bold;
  color: #333333;
  line-height: 40rpx;
  margin-bottom: 9rpx;
}
.hidden_muchword {
  width: 624rpx;
  height: 66rpx;
  font-size: 24rpx;
  line-height: 33rpx;
  display: -webkit-box;
  word-break: break-all;
  text-overflow: ellipsis;
  overflow: hidden;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
}

.list_price {
  color: #ff4652;
  display: block;
  font-size: 34rpx;
  margin-top: 16rpx;
}
.list_carlabel{
  width: 430rpx;
  height: 50rpx;
  overflow: hidden;
  line-height:45rpx
}
.list_type {
  padding: 3rpx 3rpx;
  border: 1px solid #1c8efa;
  margin-right: 10rpx;
  border-radius: 4rpx;
  margin-bottom: 5rpx;
  font-size: 20rpx;
  text-align: center;
  line-height: 30rpx;
  color: #338bff;
}
.list_top_type {
  padding: 3rpx 5rpx;
  height: 30rpx;
  background: #1c8efa;
  border-radius: 4rpx;
  font-size: 20rpx;
  text-align: center;
  line-height: 30rpx;
  margin-right: 10rpx;
  color: #fff;
}
.bottom_line {
  border-bottom: 1rpx solid #e8e8e8;
  width: 690rpx;
  margin: 40rpx auto 0;
}
.todaynewcar_bottom_line {
  border-bottom: 1rpx solid #e8e8e8;
  width: 690rpx;
  margin: 0 auto;
}
.rentoutlogo {
  background-color: #fff;
  width: 110rpx;
  height: 90rpx;
  position: absolute;
  z-index: 1000;
  right: 20rpx;
  top: 10rpx;
  opacity: 1;
}
.navbar{
  z-index: 9999;
}
.rentout_box {
  opacity: 0.5;
  margin: 0 auto;
  position: relative;
}
.nocar_list {
  width: 100%;
  height: 100%;
}
.nocar_img {
  width: 560rpx;
  height: 349rpx;
  margin: 219rpx 95rpx 31rpx;
}
.nocar_title {
  display: block;
  width: 100%;
  font-size: 28rpx;
  text-align: center;
  color: #666;
}

.top_title {
  font-size: 34rpx;
  color: #fff;
  font-weight: bold;
}
.scroll-view {
  position: absolute;
  top: 150rpx;
  bottom: 0;
  width: 100%;
  flex: 1;
}
.nocar_page {
  width: 100%;
  height: 100%;
}
