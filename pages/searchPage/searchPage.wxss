.goback_btn {
  width: 100rpx;
  height: 40rpx;
  font-size: 22rpx;
  position: absolute;
  left: 30rpx;
  top: 55rpx;
}
.search {
  position: relative;
  width: 400rpx;
  height: 58rpx;
  background: #fff;
  border-radius: 29rpx;
  color: #333;
  font-size: 24rpx;
}
.search_box {
  width: 100%;
  height: 150rpx;
  background: #2f83fa;
  position: relative;
}
.sousuo_icon {
  color: #666;
  font-size: 30rpx;
  position: absolute;
  left: 35rpx;
  top: 15rpx;
  z-index: 9999;
}
.guanbi_icon {
  font-size: 30rpx;
  position: absolute;
  top: 15rpx;
  right: 30rpx;
  color: #ccc;
  z-index: 9999;
}
.tesst_input {
  width: 100rpx;
  height: 50rpx;
  background-color: pink;
  margin-left: 20rpx;
}
.search_history_box {
  min-height: 240rpx;
  margin: 42rpx 30rpx;
}
.history_toptitle {
  width: 100%;
  height: 48rpx;
  line-height: 48rpx;
  margin-bottom: 42rpx;
}
.history_maintitle {
  display: block;
  float: left;
  font-size: 34rpx;
  color: #333;
  font-weight: bold;
}
.delete_img {
  width: 30rpx;
  height: 30rpx;
  margin-top: 3rpx;
  display: block;
  float: right;
}
.history_box {
  min-height: 150rpx;
  display: flex;
  align-items: center;
  flex-wrap: wrap;
}
.history_detail_box {
  color: #333;
  height: 60rpx;
  max-width: 650rpx;
  font-size: 24rpx;
  line-height: 60rpx;
  background: #f4f4f4;
  margin-right: 50rpx;
  padding: 0 37rpx;
  border-radius: 30rpx;
  margin-bottom: 30rpx;
}
