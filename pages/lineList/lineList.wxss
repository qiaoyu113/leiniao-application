}

.lineListBox {
  background: #edecec;
}

.lineList .lineListTop {
  width: 100%;
  position: relative;
  border-bottom: 2rpx solid #EDECEC;
}

.lineList .lineListTop .name {
  font-size: 30rpx;
  color: #000;
  font-weight: bold;
  padding: 25rpx 40rpx;
  box-sizing: border-box;
}

.lineList .lineListTop .subsidiary{
  font-size: 24rpx;
  color: #9B9B9B;
  font-weight: normal;
  margin-top:12rpx;
}

.lineList .lineListTop .income_month{
  background: #3ACB8D;
  border-radius: 92rpx 0 0 92rpx;
  width:230rpx;
  position: absolute;
  right:0;
  top: 16rpx;
  height: 55rpx;
  line-height: 55rpx;
  text-align: center;
  font-size: 22rpx;
  color: #FFFFFF;
  font-weight: normal;
}

.lineList .lineListBottom .lineListBottom_top{
  width: 100%;
  padding: 20rpx 50rpx;
  box-sizing: border-box;
  position: relative;
}

.lineList .lineListBottom .lineListBottom_top .lineListBottom_top_list{
  height:50rpx;
  line-height: 50rpx;
  font-size: 28rpx;
}

.lineList .lineListBottom .lineListBottom_top .lineListBottom_top_list span{
  font-size: 26rpx;
  color: #838383;
}

.lineList .lineListTop .center {
  width: 100%;
  overflow: hidden;
  padding-top:15rpx;
}

.lineList .lineListTop .center .center_left {
  width: 45rpx;
  float: left;
  margin-left: 55rpx;
}

.lineList .lineListTop .center .center_right {
  width: calc(100% - 100rpx);
  float: left;
  padding-left: 15rpx;
  box-sizing: border-box;
}

.lineListBottom_bottom{
  width: 100%;
  padding: 0 30rpx 30rpx;
  box-sizing: border-box;
}

.lineListBottom_bottom span{
  padding: 5rpx 28rpx;
  box-sizing: border-box;
  font-size: 24rpx;
  color: #FF7B00;
  background: #FFF4E8;
  border-radius: 6rpx;
  margin-right: 10rpx;
  margin-bottom: 8rpx;
  display:inline-block
}

