/* pages/personalCenter/personalCenter.wxss */
page{
  background: #F2F2F2;
}

.getUserInfoOver{
  position: fixed;
  z-index: 100;
  top:0;
  bottom:0;
  left:0;
  right:0;
  opacity: 0;
}

.personalHeader{
  width: 100%;
  /* height: 476rpx; */
  height: 350rpx;
  box-sizing: border-box;
  background-image: linear-gradient(180deg, #EFAB49 0%, #FFE172 100%);
  padding: 30rpx 50rpx;
  box-sizing: border-box;
}

.getUserInfo{
  border: none;
  background-color: none;
  font-size: 34rpx;
  color: #8B572A;
  margin-top: 50rpx;
}

.getUserInfo::after{
  border: none; 
  background: none;
}

.header{
  display: flex;
}

.headerLeft{
  width: 128rpx;
  height: 128rpx;
  overflow: hidden;
  border: 3px solid #fff;
  border-radius: 100%;
}

.headerRight{
  flex: 3;
  padding-left: 24rpx;
  box-sizing: border-box;
}

.headerRight .UserName{
  font-size: 34rpx;
  color: #8B572A;
  line-height: 40px;
}

.headerRight .UserName text{
  color: #FBE2A0;
  margin-left: 5rpx;
}

.headerRight .menue .isVip{
  background: #4A525C;
  border-radius: 8px;
  border-radius: 8px;
  padding: 3rpx 20rpx;
  box-sizing: border-box;
  display: inline-block;
  font-size: 26rpx;
  color: #FFD976;
  margin-right: 10rpx;
}

.headerRight .menue .isNoVip{
  background: #FDEED7;
  border-radius: 8px;
  border-radius: 8px;
  padding: 3rpx 20rpx;
  box-sizing: border-box;
  display: inline-block;
  font-size: 26rpx;
  color: #9E7C67;
}

.menuBox{
  width: 100%;
  padding: 40rpx 30rpx 30rpx;
  box-sizing: border-box;
  display: flex;
}

.menuBox .menuList{
  flex: 1;
  text-align: center;
  font-size: 34rpx;
  color: #8B572A;
}

.menuBox .menuList .num{
  line-height: 46rpx;
}

.card{
  width: 100%;
  height: 145rpx;
  background: #4A525C;
  box-shadow: 0 0 14px 6px rgba(255,160,0,0.15);
  padding: 0 30rpx;
  box-sizing: border-box;
  display: flex;
  border-top-left-radius: 12rpx;
  border-top-right-radius: 12rpx;
}

.card .cardLeft{
  flex: 1;
  text-align: left;
  font-size: 34rpx;
  color: #FFD976;
  line-height: 50rpx;
}

.card .cardLeft .info{
  font-size: 26rpx;
  color: #FDEED7;
}

.cardName{
  line-height: 50rpx;
  margin-top: 20rpx;
}

.card .cardRight{
  flex: 1;
  text-align: right;
}

.card .cardRight .buyCard{
  margin-top: 30rpx;
  background-image: linear-gradient(269deg, #E9B263 0%, #FFE172 100%);
  border-radius: 33px;
  border-radius: 33px;
  display: inline-block;
  font-size: 26rpx;
  color: #4A4A4A;
  padding: 4rpx 28rpx;
  box-sizing: border-box;
}

.mainService{
  width: 100%;
  background: #fff;
  margin-top: -10rpx;
  border-radius: 20rpx;
  /* border-top-right-radius: 20rpx; */
  padding: 0 30rpx;
  box-sizing: border-box;
}

.mainService .title{
  height: 95rpx;
  line-height: 95rpx;
  font-size: 32rpx;
  color: #4A4A4A;
  border-bottom: 2rpx solid #F2F2F2;
  padding:0 18rpx;
  box-sizing: border-box;
}

.getPhone{
  width:100%;
  position: fixed;
  top: 0;
  left:0;
  bottom:0;
  right:0;
  opacity: 0;
}

.mainSelect{
  width:100%;
  padding-bottom: 30rpx;
  overflow: hidden;
}

.mainSelect .mainSelectList{
  width: 25%;
  float: left;
  text-align: center;
  padding-top: 40rpx;
  box-sizing: border-box;
}

.mainSelect .mainSelectList .mainSelectIcon{
  width: 100rpx;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  border-radius: 100%;
  margin: auto;
  padding-top: 20rpx;
}

.mainSelect .mainSelectList .mainSelectIcon image{
  width: 100%;
}

.mainSelect .mainSelectList .mainTitle{
  width: 100%;
  font-size: 28rpx;
  color: #838383;
  text-align: center;
  margin-top:16rpx;
}

