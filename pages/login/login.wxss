/* pages/login/login.wxss */
.titleStyle{
  color:#fff;
}

.icons{
  width: 160rpx;
  margin: 25vh auto 250rpx;
}

.icons image{
  width: 160rpx;
  height: 160rpx;
}

.loginBtn{
  padding: 0 160rpx;
}

.loginBtn button{
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  background: #1C8EFA;
  border-radius: 10rpx;
  color: #fff;
  font-size: 28rpx;
}

.phoneLogin{
  width: 100%;
  color: #666666;
  font-size: 28rpx;
  text-align: center;
  line-height: 120rpx;
}

.author{
  width: 100%;
  text-align: center;
  position: absolute;
  bottom: 36rpx;
  color:#666;
  font-size: 24rpx;
}

.link{
  color: #1C8EFA;
}