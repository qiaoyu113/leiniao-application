/* pages/lineList/lineList.wxss */

.SearchBox {
  width: 100%;
  position: relative;
}

page {
  background-color: #edecec;
}

.searchInp {
  width: 100%;
  /* padding: 40rpx 30rpx; */
  box-sizing: border-box;
  position: absolute;
  top: 0;
}

.searchInp .searchLeft {
  width: 40%;
  text-align: left;
  float: left;
  height: 80rpx;
  line-height: 80rpx;
  font-size: 28rpx;
  color: #656565;
  padding-left: 45rpx;
  box-sizing: border-box;
  background: #fdeed7;
}

.searchInp .searchRight {
  width: 60%;
  float: left;
  height: 80rpx;
  background: #fdeed7;
  padding: 12rpx 0;
  box-sizing: border-box;
}

.searchInp .searchRight .li {
  width: 30%;
  float: left;
  height: 58rpx;
  line-height: 58rpx;
  font-size: 28rpx;
  color: #656565;
  text-align: center;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
  overflow: hidden;
  padding: 0 5rpx;
  box-sizing: border-box;
}

.searchInp .searchRight .li:nth-child(2){
  width:40%;
}

.borderR {
  border-right: 2rpx solid #ccc;
}

