.nav-bar {
  position: fixed;
  width: 100%;
  height: 520rpx;
  top: 0;
  color: #fff;
  text-align: center;
}
.nav-bar .nav_city {
  position: absolute;
  top: 80rpx;
  left: 30rpx;
  font-size: 26rpx;
  line-height: 15rpx;
  vertical-align: middle;
}
.nav-bar .search {
  width: 400rpx;
  height: 58rpx;
  background: rgba(255, 255, 255, 0.2);
  border-radius: 29rpx;
  position: absolute;
  top: 55rpx;
  left: 136rpx;
  color: #333;
  font-size: 24rpx;
}
.nav_title {
  font-size: 14px;
}
.title_box {
  width: 100%;
  height: 25px;
  position: absolute;
  bottom: 5px;
}
/*轮播图*/
.slides {
  width: 750rpx;
  height: 520rpx;
}
.slides swiper {
  height: 520rpx;
}
.slides navigator {
  width: 100%;
  height: 100%;
}
.slides image {
  width: 100%;
  height: 100%;
}
.banner_img {
  width: 750rpx;
  height: 520rpx;
  z-index: -1000;
}
